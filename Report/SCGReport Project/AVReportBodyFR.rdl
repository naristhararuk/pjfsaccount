  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>eAccounting</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@advanceID">
            <Value>=Parameters!advanceID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT stat.Comment + ' / ' + pb.PBCode + ' : ' + pbLang.Description AS Comment, avItem.AmountTHB ,avItem.MainCurrencyAmount,comp.companyName,avItem.currencyid,curr.Symbol AS currency
,cur.Symbol AS maincurrency,avItem.ExchangeRate,avItem.Amount, avItem.AmountTHB*1,avDoc.IsRepOffice
,isnull(cur.CurrencySymbol,'') as CurrencySymbol
FROM avAdvanceDocument avDoc 
INNER JOIN avAdvanceItem avItem
ON avDoc.AdvanceID = avItem.AdvanceID
LEFT JOIN DBStatus stat
ON avItem.PaymentType = stat.Status 
LEFT JOIN DBPB pb
ON pb.pbid = avdoc.pbid 
LEFT JOIN DBPBLang pbLang
ON pb.pbid = pbLang.pbid AND LanguageID = 1
LEFT JOIN DBCompany comp 
ON pb.companyCode  = comp.companyCode 
LEFT JOIN DBCurrency cur
