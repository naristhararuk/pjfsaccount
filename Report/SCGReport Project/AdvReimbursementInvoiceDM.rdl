  <DataSets>
    <DataSet Name="eAccounting">
      <Query>
        <DataSourceName>eAccounting1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@expenseId">
            <Value>=Parameters!expenseId.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select row_Number() over (order by invoice.InvoiceID) as Seq,invoice.InvoiceID as InvoiceID,invoice.InvoiceNo as InvoiceNo,invoice.InvoiceDate as InvoiceDate ,
invoice.TotalBaseAmountLocalCurrency as BaseAmountLocalCurrency , invoice.TotalBaseAmount as BaseAmount ,invoice.VatAmount as VatAmount, 
invoice.WHTAmount as WHTAmount, invoice.NetAmountLocalCurrency as NetAmountLocalCurrency,invoice.NetAmount as NetAmount,invoice.VendorCode as VendorCode, 
invoice.VendorName as VendorName,(case when expadvtotal.totaladvance is null then 0 else expadvtotal.totaladvance end) as totaladvance
from FnExpenseInvoice invoice
left join (
select sum(amount) as totaladvance,fn.expenseid as expenseid from avadvancedocument adv
inner join fnexpenseadvance fn on adv.advanceid = fn.advanceid
group by fn.expenseid
) expadvtotal on invoice.expenseid = expadvtotal.expenseid
where invoice.ExpenseID = @expenseId
union
select * from (
select NULL as Seq,NULL as InvoiceID, NULL as InvoiceNo,NULL as InvoiceDate,
NULL as BaseAmountLocalCurrency , NULL as BaseAmount ,NULL as VatAmount, 
NULL as WHTAmount, NULL as NetAmountLocalCurrency,NULL as NetAmount,NULL as VendorCode, 
NULL as VendorName,sum(adv.amount) as totaladvance
from  avadvancedocument adv 
inner join fnexpenseadvance fnexpadv on adv.advanceid = fnexpadv.advanceid
where fnexpadv.expenseid = @expenseId
) tt
where  (select count(1) from fnexpenseInvoice where expenseid = @expenseId) = 0</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
