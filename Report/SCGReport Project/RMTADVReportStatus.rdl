  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>eAccounting</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@RMTID">
            <Value>=Parameters!RMTID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT 
    usrCre.UserName,
    usrCre.EmployeeName,
    usrCre.PositionName,
    doc.DocumentDate,
    usrReq.UserName AS UserName_1, 
    usrReq.EmployeeName AS EmployeeName_1,
    usrReq.PositionName AS PositionName_1,
    doc.DocumentDate AS DocumentDate_1
FROM fnRemittance rmt
LEFT JOIN [Document] doc 
ON rmt.DocumentID = doc.DocumentID 
LEFT JOIN SuUser usrCre 
ON doc.CreatorID = usrCre.UserID 
LEFT JOIN SuUSer usrReq 
ON doc.RequesterID = usrReq.UserID 

WHERE rmt.RemittanceID = @RMTID</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="UserName">
          <DataField>UserName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="EmployeeName">
