  <DataSets>
    <DataSet Name="eAccounting">
      <Query>
        <DataSourceName>eAccounting1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@expenseId">
            <Value>=Parameters!expenseId.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select row_Number() OVER (ORDER BY t2.Symbol) AS Seq,paymenttype,t2.symbol,sum(positive) as advance,sum(positiveTHB) as advanceTHB, sum(positiveMainCurrency) as advanceMainCurrencyAmount ,sum(negative) as remittance,sum(negativeTHB) as remittanceTHB, sum(negativeMainCurrency) as remittanceMainCurrencyAmount, exp.NetAmount 
from (select t3.paymenttype,t3.currencyid,sum(t3.amount) as positive,sum(t3.amountthb) as positiveTHB, sum(isnull(t3.MainCurrencyAmount,0)) as positiveMainCurrency ,0 as negative,0 as negativeTHB, 0 as negativeMainCurrency
from fnexpensedocument t1
inner join fnexpenseadvance t2
on t1.expenseid = t2.expenseid
inner join avadvanceitem t3
on t2.advanceid = t3.advanceid
where t1.expenseid =  @expenseId
group by t3.paymenttype,t3.currencyid
union
select t4.paymenttype,t4.currencyid,0,0,0,sum(t4.foreigncurrencyremitted) as negative,sum(t4.amountthb) as negativeTHB, sum(isnull(t4.MainCurrencyAmount,0)) as negativeMainCurrency
from fnremittance rm
inner join fnremittanceitem t4
	on rm.remittanceid = t4.remittanceid
inner join workflow wf
	on wf.documentid = rm.documentid
	inner join workflowstate wfs
	on wf.currentstate = wfs.workflowstateid
	and wfs.name &lt;&gt; 'Cancel'
where exists
(
	select * from fnexpensedocument t1 
	inner join fnexpenseadvance t2
	on t1.expenseid = t2.expenseid 
	inner join fnremittanceadvance t3
	on t2.advanceid = t3.advanceid
	where rm.remittanceid = t3.remittanceid
and t1.expenseid = @expenseId
)
group by t4.paymenttype,t4.currencyid
) t1 
inner join dbcurrency t2
on t2.currencyid = t1.currencyid
INNER JOIN (SELECT (case when isnull(IsRepOffice,0) = 0 then FnExpenseDocument.DifferenceAmount else FnExpenseDocument.DifferenceAmountLocalCurrency end)  AS NetAmount 
FROM         FnExpenseDocument
WHERE    FnExpenseDocument.Active = 1 AND FnExpenseDocument.ExpenseID = @expenseId) exp 
on 1=1 
group by t1.paymenttype,t2.symbol,exp.NetAmount</CommandText>
