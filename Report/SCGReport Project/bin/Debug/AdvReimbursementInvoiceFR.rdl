  <DataSets>
    <DataSet Name="eAccounting">
      <Query>
        <DataSourceName>eAccounting1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@expenseId">
            <Value>=Parameters!expenseId.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT     row_Number() OVER (ORDER BY invoice.InvoiceID) AS Seq, invoice.InvoiceID AS InvoiceID, invoice.InvoiceNo AS InvoiceNo, 
invoice.InvoiceDate AS InvoiceDate,  invoice.TotalBaseAmount as BaseAmount , invoice.TotalBaseAmountLocalCurrency as BaseAmountLocal,invoice.TotalBaseAmount as BaseAmount, invoice.VatAmount AS VatAmount,
invoice.WHTAmount AS WHTAmount , invoice.NetAmount AS NetAmount,
invoice.NetAmountLocalCurrency AS NetAmountLocalCurrency, invoice.VendorCode AS VendorCode, invoice.VendorName AS VendorName
FROM         FnExpenseInvoice invoice
WHERE     invoice.Active = 1 AND invoice.ExpenseID = @expenseId
union
select NULL, NULL, NULL, NULL, NULL, NULL, NULL, NULL, NULL, NULL,NULL,NULL,NULL
where  (select count(1) from fnexpenseInvoice where expenseid = @expenseId) = 0</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
