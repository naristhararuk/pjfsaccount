    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>eAccounting</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@MPADOC">
            <Value>=Parameters!MPADOC.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT comp.companyName,comp.companyCode,doc.DocumentNo,doc.DocumentDate,suuser.employeename,suuser.employeecode,suuser.sectionname,suuser.PhoneNo,suuser.MobilePhoneNo,doc.Subject,mpaDoc.MPADocumentID,mpaDoc.StartDate , mpaDoc.EndDate
FROM MPADocument mpaDoc  
LEFT JOIN [Document] doc 
ON mpaDoc.DocumentID = doc.DocumentID 
LEFT JOIN DbCompany comp 
ON comp.CompanyID = doc.CompanyID 
LEFT JOIN SuUser suuser 
ON Doc.RequesterID = suuser.UserID 
where mpaDoc.DocumentID = @MPADOC</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
