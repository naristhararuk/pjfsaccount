      <Query>
        <DataSourceName>eAccounting1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@expenseId">
            <Value>=Parameters!expenseId.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select row_Number() over (order by invoice.InvoiceID) as Seq,  invoice.InvoiceID as InvoiceID, invoice.InvoiceNo as InvoiceNo
,invoice.InvoiceDate as InvoiceDate ,invoice.TotalBaseAmountLocalCurrency as BaseAmountLocalCurrency,invoice.TotalBaseAmount as BaseAmount
,invoice.VatAmount as VatAmount, invoice.WHTAmount as WHTAmount, invoice.NetAmountLocalCurrency as NetAmountLocalCurrency, invoice.NetAmount as NetAmount
,invoice.VendorCode as VendorCode, invoice.VendorName as VendorName
from  FnExpenseInvoice invoice
where invoice.Active = 1 and invoice.ExpenseID = @expenseId order by invoice.InvoiceNo</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
