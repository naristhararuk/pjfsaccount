  <DataSets>
    <DataSet Name="eAccounting">
      <Query>
        <DataSourceName>eAccounting1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@CurrencyID">
            <Value>=Parameters!CurrencyID.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@invoiceId">
            <Value>=Parameters!invoiceId.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select item.InvoiceItemID as InvoiceItemID,  item.Description as ItemDescription, item.CurrencyAmount as CurrencyAmount,item.ExchangeRate as ExchangeRate, item.LocalCurrencyAmount as LocalAmount , item.Amount as Amount
,cost.CostCenterCode as CostCenterCode, cost.Description as CostDescription
,ac.AccountCode as AccountCode, acl.AccountName as AccountName
,internal.IONumber as OrderNo
,currency.Symbol as Currency
,curr.Symbol as LocalCurrency
from FnExpenseInvoiceItem item
left join DbCostCenter cost on cost.CostCenterID = item.CostCenterID
inner join DbAccount ac on ac.AccountID = item.AccountID
