  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>eAccounting</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@RemittanceID">
            <Value>=Parameters!RemittanceID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT  row_Number() OVER (ORDER BY rmtadv.RemittanceAdvanceID) AS Seq,doc.documentNo,doc.Subject,advDoc.DueDateOfRemittance,advDoc.amount ,ISNULL(advDoc.IsRepOffice,0) as IsRepOffice,advDoc.MainCurrencyAmount,DbCurrency.Symbol, DbCurrency.CurrencySymbol
,DbCurrency.CurrencyID
FROM FnRemittanceAdvance rmtadv 
INNER JOIN avAdvanceDocument advDoc
ON advDoc.AdvanceID = rmtadv.AdvanceID 
INNER JOIN [DOCUMENT] doc
ON doc.DocumentID = advDoc.DocumentID 
LEFT JOIN DbCurrency
ON advDoc.MainCurrencyID =DbCurrency.CurrencyID 

WHERE rmtadv.RemittanceID =@RemittanceID</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="Seq">
          <DataField>Seq</DataField>
          <rd:TypeName>System.Int64</rd:TypeName>
        </Field>
        <Field Name="DocumentNo">
