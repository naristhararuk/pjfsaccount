          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>eAccounting</DataSourceName>
        <CommandText>SELECT comp.CompanyCode,comp.CompanyName,
doc.DocumentNo,bap.FI_DOC,
usr.EmployeeName,usr.EmployeeCode,usr.SectionName,
usrRec.EmployeeName as receivername,usrRec.EmployeeCode as recievercode,usrRec.SectionName as receiverSectioname,
avDoc.RequestDateOfAdvance,
avDoc.DueDateOfRemittance,
Doc.Subject ,Doc.DocumentDate

FROM avAdvanceDocument avDoc 

LEFT JOIN [Document] doc
ON avDoc.DocumentID = doc.DocumentID 
LEFT JOIN DbCompany comp
ON doc.CompanyID = comp.CompanyID 
LEFT JOIN BAPIACHE09 bap
ON bap.DOC_ID = doc.DocumentID 
LEFT JOIN SuUSer usr 
ON doc.RequesterID = usr.UserID 
LEFT JOIN SuUser usrRec 
ON doc.ReceiverID = usrRec.UserID

WHERE avDoc.advanceID = @advanceID</CommandText>
        <QueryParameters>
          <QueryParameter Name="@advanceID">
            <Value>=Parameters!advanceID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
