  <DataSets>
    <DataSet Name="DataSet1">
      <Fields>
        <Field Name="Seq">
          <DataField>Seq</DataField>
          <rd:TypeName>System.Int64</rd:TypeName>
        </Field>
        <Field Name="DocumentNo">
          <DataField>documentNo</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Subject">
          <DataField>Subject</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="DueDateOfRemittance">
          <DataField>DueDateOfRemittance</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="amount">
          <DataField>amount</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>eAccounting</DataSourceName>
        <CommandText>SELECT  row_Number() OVER (ORDER BY rmtadv.RemittanceAdvanceID) AS Seq,doc.documentNo,doc.Subject,advDoc.DueDateOfRemittance,advDoc.amount 
FROM FnRemittanceAdvance rmtadv 
INNER JOIN avAdvanceDocument advDoc
ON advDoc.AdvanceID = rmtadv.AdvanceID 
INNER JOIN [DOCUMENT] doc
ON doc.DocumentID = advDoc.DocumentID 

WHERE rmtadv.RemittanceID =@RemittanceID</CommandText>
        <QueryParameters>
          <QueryParameter Name="@RemittanceID">
            <Value>=Parameters!RemittanceID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
  </DataSets>
