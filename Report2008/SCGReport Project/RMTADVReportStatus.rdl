        </Field>
        <Field Name="UserName_1">
          <DataField>UserName_1</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="EmployeeName_1">
          <DataField>EmployeeName_1</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="PositionName_1">
          <DataField>PositionName_1</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="DocumentDate_1">
          <DataField>DocumentDate_1</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>eAccounting</DataSourceName>
        <CommandText>SELECT usrCre.UserName,usrCre.EmployeeName,usrCre.PositionName,doc.DocumentDate,
 usrReq.UserName AS UserName_1,usrReq.EmployeeName AS EmployeeName_1,usrReq.PositionName AS PositionName_1,doc.DocumentDate
 AS DocumentDate_1 FROM fnRemittance rmt
LEFT JOIN [Document] doc 
ON rmt.DocumentID = doc.DocumentID 
LEFT JOIN SuUser usrCre 
ON doc.CreatorID = usrCre.UserID 
LEFT JOIN SuUSer usrReq 
ON doc.RequesterID = usrReq.UserID 

WHERE rmt.RemittanceID = @RMTID</CommandText>
        <QueryParameters>
          <QueryParameter Name="@RMTID">
            <Value>=Parameters!RMTID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
