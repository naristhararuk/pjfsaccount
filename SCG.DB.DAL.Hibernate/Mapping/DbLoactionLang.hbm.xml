<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2">
	<class name="SCG.DB.DTO.DbLocationLang,SCG.DB.DTO" table="DbLocationLang">

		<id name="LocationLangID" column="LocationLangID" type="Int64" unsaved-value="0">
			<generator class="native"/>
		</id>
		<many-to-one name="LanguageID" column="LanguageID" class="SS.DB.DTO.DbLanguage,SS.DB.DTO" />
		<many-to-one name="LocationID" column="LocationID" class="SCG.DB.DTO.DbLocation,SCG.DB.DTO" />
		<property column="LocationName" type="String" name="LocationName" length="100"/>
		<property column="Comment" type="String" name="Comment" length="500"/>
		<property column="UpdBy" type="Int64" name="UpdBy" not-null="true" />
		<property column="UpdDate" type="DateTime" name="UpdDate" not-null="true" />
		<property column="CreBy" type="Int64" name="CreBy" not-null="true" update="false"  />
		<property column="CreDate" type="DateTime" name="CreDate" not-null="true" update="false"  />
		<property column="UpdPgm" type="String" name="UpdPgm" not-null="true" length="30" />
		<property column="RowVersion" type="Byte[]" name="RowVersion"  insert="false" update="false"  />
		<property column="Active" type="Boolean" name="Active" not-null="true" />

	</class>
</hibernate-mapping>
