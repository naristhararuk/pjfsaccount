<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2">
	<class name="SS.SU.DTO.SuProgram,SS.SU.DTO" table="SuProgram">

		<id name="Programid" column="ProgramID" type="Int16" unsaved-value="0">
			<generator class="native"/>
		</id>
		<property column="ProgramCode" type="String" name="ProgramCode" length="50"/>
		<property column="ProgramPath" type="String" name="ProgramPath" length="350"/>
		<property column="Comment" type="String" name="Comment" length="500" />
		<property column="UpdBy" type="Int64" name="UpdBy" not-null="true" />
		<property column="UpdDate" type="DateTime" name="UpdDate" not-null="true" />
		<property column="CreBy" type="Int64" name="CreBy" not-null="true" />
		<property column="CreDate" type="DateTime" name="CreDate" not-null="true" />
		<property column="UpdPgm" type="String" name="UpdPgm" not-null="true" length="30" />
		<property column="RowVersion" type="Byte[]" name="RowVersion" insert="false" update="false" />
		<property column="Active" type="Boolean" name="Active" not-null="true" />
		
	</class>
</hibernate-mapping>
